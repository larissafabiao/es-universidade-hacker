<h1>login</h1>

<%= form_with scope: :usuario, url: login_path, local: true do |form| %>
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>

  <p>
    <%= form.label :senha %><br>
    <%= form.text_field :senha %>
  </p>

  <p>
    <%= form.submit 'Entrar' %>
  </p>
<% end %>